{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Version": "2012-10-17",
            "Statement": [
                {
                    "Sid": "SpotDataAccess",
                    "Effect": "Allow",
                    "Action": [
                        "s3:GetBucketLocation",
                        "s3:ListBucket",
                        "s3:GetObject"
                    ],
                    "Resource": [
                        "arn:aws:s3:::SpotDataFeedBucketName",
                        "arn:aws:s3:::SpotDataFeedBucketName/*"
                    ]
                }
            ]
        }
    ]
}